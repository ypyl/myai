{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (Console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net8.0/myai.dll", // Adjust according to your project's target framework and path
            "args": ["code"], // Passing 'code' as a parameter
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "logging": {
                "moduleLoad": false
            }
        }
    ]
}
